@model IEnumerable<AgendamentoConsultas.Models.Anamnese.AnamneseEntity>



    <p>
        <a href="@Url.Action("Create")" class="btn btn-success">
            <span class="glyphicon gliphicon-plus"></span>
            Novo Anaminese
        </a>

    </p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Sintomas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DoencasPreExistentes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ParteDoCorpo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Sintomas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DoencasPreExistentes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParteDoCorpo.Nome)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
<nav>
    <ul class="pager">
        <li><a href="#" onclick="paginar(-1)">Anterior</a></li>
        <li><a href="#" onclick="paginar(1)">Próxima</a></li>
    </ul>
</nav>
